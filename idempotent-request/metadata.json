{
    "name": "Idempotent Request Agent",
    "description": "An agent that performs idempotent HTTP POST requests to a specified URL, including the Request ID in the payload.",
    "image": "https://picsum.photos/300/300",
    "container_image": "https://storage.googleapis.com/my-public-stuff/idempotent-request-agent.tar",
    "version": "1.0.0",
    "author": "Antigravity",
    "methods": [
        {
            "name": "request",
            "description": "Performs an HTTP POST request with the given key-value pairs and the Somnia Request ID.",
            "inputs": [
                {
                    "name": "url",
                    "type": "string"
                },
                {
                    "name": "keys",
                    "type": "string[]"
                },
                {
                    "name": "values",
                    "type": "string[]"
                }
            ],
            "outputs": [
                {
                    "name": "response",
                    "type": "string"
                }
            ]
        }
    ],
    "tags": [
        "utility",
        "http",
        "post",
        "idempotent"
    ],
