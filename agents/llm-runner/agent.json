{
  "agentId": "12847293847561029384",
  "name": "LLM Inference",
  "description": "A simple string-in-string-out LLM inference agent using Qwen3-30B",
  "version": "1.0.0",
  "author": "Somnia Team",
  "abi": [
    {
      "type": "function",
      "name": "inferSingle",
      "inputs": [
        { "name": "prompt", "type": "string" },
        { "name": "system", "type": "string" },
        { "name": "temperature", "type": "uint256" },
        { "name": "seed", "type": "uint256" }
      ],
      "outputs": [
        { "name": "response", "type": "string" }
      ]
    },
    {
      "type": "function",
      "name": "inferConversation",
      "inputs": [
        { "name": "roles", "type": "string[]" },
        { "name": "messages", "type": "string[]" },
        { "name": "temperature", "type": "uint256" },
        { "name": "seed", "type": "uint256" }
      ],
      "outputs": [
        { "name": "response", "type": "string" }
      ]
    }
  ],
  "tags": ["llm", "inference", "ai"]
}
